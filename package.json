{
	"name": "syntax-highlighter",
	"displayName": "Syntax Highlighter",
	"description": "Syntax highlighting based on Tree-sitter",
	"homepage": "https://github.com/EvgeniyPeshkov/syntax-highlighter",
	"version": "0.2.6",
	"publisher": "evgeniypeshkov",
	"license": "MIT",
	"icon": "images/syntax.png",
	"repository": {
		"url": "https://github.com/EvgeniyPeshkov/syntax-highlighter"
	},
	"author": {
		"name": "Evgeniy Peshkov",
		"email": "peshkov.e@outlook.com"
	},
	"categories": [
		"Programming Languages",
		"Themes",
		"Other"
	],
	"keywords": [
		"Syntax",
		"Highlighter",
		"Highlight",
		"Highlighting",
		"Coloring",
		"Tree-Sitter",
		"Parser",
		"C",
		"C++",
		"CPP",
		"Python",
		"TypeScript",
		"JavaScript",
		"Go",
		"Rust"
	],
	"engines": {
		"vscode": "^1.15.0"
	},
	"activationEvents": [
		"onLanguage:c",
		"onLanguage:cpp",
		"onLanguage:python",
		"onLanguage:typescript",
		"onLanguage:javascript",
		"onLanguage:go",
		"onLanguage:rust"
	],
	"main": "./out/extension.js",
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install"
	},
	"dependencies": {
		"jsonc-parser": "^2.1.0",
		"web-tree-sitter": "^0.15.2"
	},
	"devDependencies": {
		"@types/node": "^12.0.2",
		"tslint": "^5.16.0",
		"typescript": "^3.4.5",
		"vscode": "^1.1.34",
		"vsce": "^1.62.0",
		"tree-sitter": "^0.15.1",
		"tree-sitter-cli": "^0.14.7",
		"tree-sitter-cpp": "^0.13.3",
		"tree-sitter-c": "^0.13.13",
		"tree-sitter-python": "^0.14.0",
		"tree-sitter-typescript": "^0.13.6",
		"tree-sitter-javascript": "^0.13.10",
		"tree-sitter-go": "^0.13.3",
		"tree-sitter-rust": "^0.13.7"
	},
	"contributes": {
		"colors": [
			{
				"id": "syntax.type",
				"description": "Color for types (class, struct, enum)",
				"defaults": {
					"dark": "#26A69A",
					"light": "#26A69A",
					"highContrast": "#26A69A"
				}
			},
			{
				"id": "syntax.scope",
				"description": "Color for scopes (namespace, module, outer class)",
				"defaults": {
					"dark": "#00897B",
					"light": "#00897B",
					"highContrast": "#00897B"
				}
			},
			{
				"id": "syntax.function",
				"description": "Color for functions (function, method, interface)",
				"defaults": {
					"dark": "#00BCD4",
					"light": "#00BCD4",
					"highContrast": "#00BCD4"
				}
			},
			{
				"id": "syntax.variable",
				"description": "Color for variables (variable, field, property)",
				"defaults": {
					"dark": "#42A5F5",
					"light": "#42A5F5",
					"highContrast": "#42A5F5"
				}
			},
			{
				"id": "syntax.string",
				"description": "Color for strings (string, regex, char)",
				"defaults": {
					"dark": "#90A4AE",
					"light": "#90A4AE",
					"highContrast": "#90A4AE"
				}
			},
			{
				"id": "syntax.number",
				"description": "Color for numbers (10, 10.0f)",
				"defaults": {
					"dark": "#90A4AE",
					"light": "#90A4AE",
					"highContrast": "#90A4AE"
				}
			},
			{
				"id": "syntax.punctuation",
				"description": "Color for punctuation (., :, {)",
				"defaults": {
					"dark": "#A1887F",
					"light": "#A1887F",
					"highContrast": "#A1887F"
				}
			},
			{
				"id": "syntax.comment",
				"description": "Color for comments",
				"defaults": {
					"dark": "#546E7A",
					"light": "#546E7A",
					"highContrast": "#546E7A"
				}
			},
			{
				"id": "syntax.constant",
				"description": "Color for constants (true, nullptr, nil)",
				"defaults": {
					"dark": "#A89F9B",
					"light": "#A89F9B",
					"highContrast": "#A89F9B"
				}
			},
			{
				"id": "syntax.directive",
				"description": "Color for directives (#include, import, use)",
				"defaults": {
					"dark": "#7E57C2",
					"light": "#7E57C2",
					"highContrast": "#7E57C2"
				}
			},
			{
				"id": "syntax.control",
				"description": "Color for control flow (if, continue, return)",
				"defaults": {
					"dark": "#7986CB",
					"light": "#7986CB",
					"highContrast": "#7986CB"
				}
			},
			{
				"id": "syntax.operator",
				"description": "Color for operators (+=, ->, ::)",
				"defaults": {
					"dark": "#9575CD",
					"light": "#9575CD",
					"highContrast": "#9575CD"
				}
			},
			{
				"id": "syntax.modifier",
				"description": "Color for modifiers (const, public, override)",
				"defaults": {
					"dark": "#00897B",
					"light": "#00897B",
					"highContrast": "#00897B"
				}
			}
		]
	}
}
